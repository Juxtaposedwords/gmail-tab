{
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "gmail tab fix"
  },
  "content_scripts": [
    {
      "js": [
        "replacer.js"
      ],
      "matches": [
        "https://mail.google.com/*"
      ],
      "run_at": "document_end",
        "permissions": [
        "tabs", "https://mail.google.com/*"
      ]
    }
  ],
  "description": "This extension will allow you to custom Gmail tabs",
  "manifest_version": 2,
  "name": "Gmail Tabs",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "options_ui": {
    "chrome_style": true,
    "page": "options.html",
    "open_in_tab": false
  },
  "version": "1.0"
}
